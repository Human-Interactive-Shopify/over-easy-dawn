        "d92c32c8-7744-40ba-a781-6c0e360b8608": {
          "type": "testimonials",
          "settings": {
            "image": "shopify:\/\/shop_images\/Frame_174_1.jpg",
            "text": "I was immediately impressed when I tried Over Easy bars - not only were they delicious, but with quality ingredients and balanced nutrients I feel good eating and recommending them.",
            "name": "April Verdi, MPH, RD, LD",
            "position": "Clinical Nutrition Manager"
          }
        },
        "f4a2657d-497c-456e-b8c0-0f5c9d2d2e24": {
          "type": "testimonials",
          "settings": {
            "image": "shopify:\/\/shop_images\/44905fee151bb375335371a007637ee0.png",
            "text": "I love that Over Easy bars have quality ingredients with a balance of carbohydrates, protein, fat, and vitamins and minerals to start the day. Plus, they have the added bonus of being both convenient and delicious.",
            "name": "Amy Gibbs, MEd, RD, LD",
            "position": "Cleveland Clinic, Director, Wellness Enterprise"
          }
        }
      },
