    "oe-testimonials": {
      "type": "oe-testimonials",
      "blocks": {
        "66dc171b-c8b1-44d6-bf0a-974a99e7839a": {
          "type": "testimonials",
          "settings": {
            "image": "shopify:\/\/shop_images\/Adam.jpg",
            "text": "When I first tried Over Easy, I absolutely loved the taste and was surprised to look at how clean and simple the ingredients were... All of my teammates love the bars, too.",
